0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	go:
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- 1.3
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- 1.4
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- 1.5
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- 1.6
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- 1.7
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- tip
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	before_script:
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- export PATH=$HOME/.local/bin:$PATH
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	before_install:
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	# Install encrypted gitcookies to get around bandwidth-limits
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	# that is causing Travis-CI builds to fail. For more info, see
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	# https://github.com/golang/go/issues/12933
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- openssl aes-256-cbc -K $encrypted_1528c3c2cafd_key -iv $encrypted_1528c3c2cafd_iv -in .gitcookies.sh.enc -out .gitcookies.sh -d || true
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- bash .gitcookies.sh || true
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- go get github.com/wadey/gocovmerge
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- go get github.com/mattn/goveralls
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- go get golang.org/x/tools/cmd/cover || true
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- go get code.google.com/p/go.tools/cmd/cover || true
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- pip install cram --user `whoami`
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	script:
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- go test . -v -covermode=count -coverprofile=profile.cov
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- go test ./cipher -v -covermode=count -coverprofile=cipher/profile.cov
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- go test ./json -v # no coverage for forked encoding/json package
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- cd jose-util && go build && PATH=$PWD:$PATH cram -v jose-util.t
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- cd ..
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	after_success:
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- gocovmerge *.cov */*.cov > merged.coverprofile
0ba55c488eaa2bd59fa46ecc0263ca44109d2949;;	- $HOME/gopath/bin/goveralls -coverprofile merged.coverprofile -service=travis-ci
