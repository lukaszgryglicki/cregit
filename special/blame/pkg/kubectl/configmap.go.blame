0000000000000000000000000000000000000000;;		configMap.Data = map[string]string{}
0000000000000000000000000000000000000000;;		if len(s.FileSources) > 0 {
0000000000000000000000000000000000000000;;			if err := handleConfigMapFromFileSources(configMap, s.FileSources); err != nil {
0000000000000000000000000000000000000000;;				return nil, err
