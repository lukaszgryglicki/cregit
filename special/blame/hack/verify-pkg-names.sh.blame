0000000000000000000000000000000000000000;;	  echo "To fix these errors, do not use capitalized or underlined characters"
0000000000000000000000000000000000000000;;	  echo "in pkg aliases. Refer to https://blog.golang.org/package-names for more info."
0000000000000000000000000000000000000000;;	  exit 1
0000000000000000000000000000000000000000;;	fi
